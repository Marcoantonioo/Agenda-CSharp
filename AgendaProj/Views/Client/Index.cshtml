<div class="container">
    <table class="table">
        <thead>
            <tr>        
                <th>Cliente</th>
                <th>Nome Cliente</th>
                <th>Email</th>
                <th>Telefone</th>
            </tr>
            </tr>
        </thead>

        @foreach(var item in Model)
        {
            <tr>
                <td>#@item.id</td>
                <td>@item.nome</td>
                <td>@item.email</td>
                <td>@item.fone</td>


                <td>
									
									<td class="col-md-1"><a class="btn btn-primary" href="client/Update/@item.id">Editar</a></td>
                        <td class="col-md-1"><a class="btn btn-danger" href="client/Delete/@item.id">Deletar</a></td>
								</td>
            </tr>
            
        }

    </table>
    
</div>